' =========================================================
' 8. Bitwise Operator
' =========================================================

A = %0101  
B = %0011  

PRINT "A = 5 (%0101), B = 3 (%0011)"
PRINT "-----------------------------"
PRINT "A BAND B: "; A BAND B  ' %0101 AND %0011 = %0001 -> 1
PRINT "A BOR B:  "; A BOR B   ' %0101 OR  %0011 = %0111 -> 7
PRINT "A BXOR B: "; A BXOR B  ' %0101 XOR %0011 = %0110 -> 6
PRINT
