objXL = CREATEOBJECT("Excel.Application")

' Access a property
objXL.Visible = TRUE



' Call a method
wb = objXL.Workbooks.Add()

' Set a cell value
objcell = objXL.ActiveSheet.Cells(1, 1)
objcell.Value = "Hello from NeReLa Basic!"

' Quit Excel
STOP
objXL.Quit()

PRINT "Excel automation complete."